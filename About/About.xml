	<!-- Package IDs of mods which should be loaded after the mod. -->
	<!-- <loadBefore/> -->

	<!-- Package IDs of mods which should be loaded before the mod. -->
	<loadAfter>
		<li>brrainz.harmony</li>
	</loadAfter>

	<!-- Package IDs of mods which are incompatible with the mod. -->
