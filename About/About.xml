<?xml version="1.0" encoding="UTF-8" ?>

<!-- This file is used to provide RimWorld with basic information about the mod. -->

<ModMetaData>
	<!-- The name of the mod. -->
	<name>Mod Template</name>

	<!-- The name(s) of the author(s) of the mod. -->
	<author>Lakuna</author>

	<!-- A URL for the mod. -->
	<url>https://github.com/Lakuna/RimWorld-Mod-Template</url>

	<!-- A unique string to identify the mod by. Typically uses the form "AuthorName.ModSeries.ModName". -->
	<packageId>Lakuna.ModTemplate</packageId>

	<!-- A list of RimWorld versions that the mod works with. -->
	<supportedVersions>
		<li>1.3</li>
	</supportedVersions>

	<!-- An outdated tag used to specify the RimWorld version that the mod works with. -->
	<!-- <targetVersion/> -->

	<!-- A description of the mod. -->
	<description>A template for creating RimWorld mods.</description>

	<!-- A list of mods which the mod requires to function. -->
	<modDependencies>
		<li>
			<!-- The package ID declared in the mod's About.xml file. -->
			<packageId>brrainz.harmony</packageId>

			<!-- A display name for the mod. -->
			<displayName>Harmony</displayName>

			<!-- The link to download the mod from on the Steam Workshop. -->
			<steamWorkshopUrl>steam://url/CommunityFilePage/2009463077</steamWorkshopUrl>

			<!-- The link to download the mod from off of the Steam Workshop. -->
			<downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
		</li>
	</modDependencies>

	<!-- If the dependencies are not the same for every version, use the following form instead. -->
